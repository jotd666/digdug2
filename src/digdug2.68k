play_sound:
    move.l  d0,-(a7)
    move.l  a0,d0
    sub.l   a6,d0
    sub.w   #sound_level_start_tune_4040,d0
	jeq		0f
	* not start music. Are we trying to cut start music?
	tst.b	(sound_level_start_tune_4040,a6)
	jne		1f				| don't interrupt level start tune
0:
    jbsr    osd_sound_start
1:
    move.l  (a7)+,d0
    rts

stop_sound:
    move.l  d0,-(a7)
    move.l  a0,d0
    sub.l   a6,d0
    sub.w   #sound_level_start_tune_4040,d0
	and.b	#0x1F,d0	| probably not useful

    lea		music_table(pc),a1
	tst.b	(a1,d0.w)
	jeq		0f
	jbsr	osd_music_stop
0:
    move.l  (a7)+,d0
    rts

* < (a0): previous value
* < d0: new value
* > (a0): possibly updated value
* (and if d0 != (a0) does something else does nothing)
*
* we need to know what was the previous value because the game
* tries to stop non-playing tunes when another tune is playing
* and our host API only has a global "stop_music" call
sound_control:
	cmp.b	(a0),d0
	jeq		1f		| already in the correct state: do nothing
    move.b	d0,(a0)	| change value
    jeq     0f		| if changes to 0 stop sound if this tune was playing
	* if changes to 1, play sound
    jbra    play_sound
0:
	jbra	stop_sound
1:
	rts
	
